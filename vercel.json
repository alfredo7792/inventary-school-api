    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_KEY": "base64:ekhGNJXhrzz3Q8RMRLegJy7+vTPjsFVxrnqcdI/G4tA=",

        "APP_URL": "https://api-inventary-school.vercel.app",
